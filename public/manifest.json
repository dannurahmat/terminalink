{
  "manifest_version": 3,
  "name": "Terminalink",
  "version": "1.0.1",
  "description": "Reduce memory use use and Organize your link collection easily",
  "icons": {
      "16": "images/ic_terminalink-alt2-16.png",
      "32": "images/ic_terminalink-alt2-32.png",
      "48": "images/ic_terminalink-alt2-48.png",
      "128": "images/ic_terminalink-alt2-128.png"
  },
  "action": {
      "default_popup": "index.html",
      "default_icon": {
          "16": "images/ic_terminalink-alt2-16.png",
          "32": "images/ic_terminalink-alt2-32.png",
          "48": "images/ic_terminalink-alt2-48.png",
          "128": "images/ic_terminalink-alt2-128.png"
      }
    },
  "author": "drahmat@terminalink.com", 
  "permissions": [
     "tabs", 
     "activeTab"
  ],
  "content_scripts": [
    {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["./static/js/content.js"]
    }
 ],
 "chrome_url_overrides" : {
  "newtab": "index.html"
}
}
